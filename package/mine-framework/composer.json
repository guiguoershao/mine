    "league/flysystem": "^1.0",
    "overtrue/wechat": "~4.0",
    "guzzlehttp/guzzle": "~6.0",
    "jaeger/querylist": "^4.0",
    "paquettg/php-html-parser": "^1.7",
    "catfan/Medoo": "^2.1.*",
    "zhuzhichao/ip-location-zh": "^2.4",
    "predis/predis": "^1.1"
