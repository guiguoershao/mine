        {
            "name": "guiguoershao",
            "email": "guiguoershao@163.com"
        }
    ],
    "require": {
        "symfony/var-dumper": "^4.2",
        "monolog/monolog": "^1.24",
        "predis/predis": "^1.1",
        "easyswoole/easyswoole": "3.x-dev"
    },
    "autoload": {
